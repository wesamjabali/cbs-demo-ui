  <v-text-field\n            v-model=\"password\"\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"password\"\n            :rules=\"passwordRules\"\n          ></v-text-field>\n            <v-text-field\n            v-model=\"payrate\"\n            name=\"payrate\"\n            type=\"number\"\n            placeholder=\"pay rate ($/hr)\"\n            :rules=\"payrateRules\"\n            required\n          ></v-text-field>\n          <v-label>Special Roles:</v-label>\n          <v-checkbox v-model=\"roles\" label=\"Super\" value=\"super\"></v-checkbox>\n          <div class=\"py-4\">\n            <v-btn :disabled=\"!valid\" type=\"submit\" color=\"primary\" block>register</v-btn>\n          </div>\n        </v-form>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\nexport default {\n    name: \"Register\",\n  data() {\n    return {\n      username: \"\",\n      email: \"\",\n      password: \"\",\n      roles: [],\n      payrate: \"\",\n      registered: false,\n\n      valid: false,\n      usernameRules: [\n        v => !!v || 'Username is required',\n      ],\n      emailRules: [\n        v => !!v || 'E-mail is required',\n        v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\n      ],\n      passwordRules: [\n        v => !!v || 'Password is required',\n        v => (v && v.length >= 6) || 'Password must be greater than 6 characters',\n      ],\n      payrateRules: [\n          \n          v => (v < 8) || 'Pay Rate must be greater than 8'\n      ]\n    };\n  },\n  methods: {\n    async register() {\n      if(!this.$refs.registerForm.validate()) return\n\n      try {\n        if(this.roles.indexOf(\"associate\") === -1)\n            this.roles.push(\"associate\");\n        let username = this.username.toLowerCase();\n        let email = this.email;\n        let password = this.password;\n        let payrate = this.payrate;\n        let roles = this.roles;\n        await this.$axios.post(\"/auth/register\", { username, email, password, payrate, roles });\n        this.registered = true;\n        } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n};\n</script>\n\n<style></style>\n"]}]}